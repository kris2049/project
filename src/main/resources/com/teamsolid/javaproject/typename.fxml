<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="267.0" prefWidth="416.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.teamsolid.javaproject.StartButtonController">
    <children>
        <TextField fx:id="player1Name" layoutX="122.0" layoutY="117.0" promptText="Enter Player 1 Name" />
        <TextField fx:id="player2Name" layoutX="122.0" layoutY="187.0" promptText="Enter Player 2 Name" />
        <Label layoutX="91.0" layoutY="59.0" text="Please enter the names of both players" />
        <Button layoutX="315.0" layoutY="219.0" mnemonicParsing="false" onAction="#handleStartGame" text="Start!">
        </Button>
        <Button layoutX="288.0" layoutY="147.0" mnemonicParsing="false" onAction="#someMethod" text="Finish Entering" />
    </children>
</AnchorPane>
